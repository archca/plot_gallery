	author = {Roux, Valentine and Bril, Blandine and Karasik, Avshalom},
	month = dec,
	year = {2018},
	keywords = {Ethnoarchaeology, Innovation, Borrowing, Ceramic techniques, Expertise, Field experiment, Weak ties},
	pages = {1024--1050},
	file = {Roux et al_2018_Weak Ties and Expertise.pdf:H\:\\BackUp\\Dokumente\\Archäologie\\Zotero\\storage\\UX49HDPW\\Roux et al_2018_Weak Ties and Expertise.pdf:application/pdf}
}

@article{drost_toy_2018,
	title = {Toy {Story}: {Homophily}, {Transmission} and the {Use} of {Simple} {Simulation} {Models} for {Assessing} {Variability} in the {Archaeological} {Record}},
