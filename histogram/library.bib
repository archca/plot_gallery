	keywords = {Data models, Diffusion of ancient cultural traits, Formal methods, Social networks, Sociological theories},
	pages = {967--973},
	file = {Roux_Manzo_2018_Social Boundaries and Networks in the Diffusion of Innovations.pdf:H\:\\BackUp\\Dokumente\\Archäologie\\Zotero\\storage\\ZFFW76SF\\Roux_Manzo_2018_Social Boundaries and Networks in the Diffusion of Innovations.pdf:application/pdf}
}

@article{roux_weak_2018,
