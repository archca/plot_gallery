@article{vela_social_2017,
	title = {Social {Networks} in {Late} {Hellenistic} {Northern} {Etruria}: {From} a multicultural society to a society of partial identities},
	volume = {3},
	copyright = {Copyright (c) 2017 Digital Classics Online},
	issn = {2364-7957},
	shorttitle = {Social {Networks} in {Late} {Hellenistic} {Northern} {Etruria}},
	url = {https://journals.ub.uni-heidelberg.de/index.php/dco/article/view/39433},
	doi = {10.11588/dco.2017.0.39433},
	abstract = {This contribution concerns the perception of local cultural identities in Hellenistic Northern Etruria. I aim to answer the following question: how did the evolution of the economic and politic relationships between settlements condition the perception and the expression of the local identities? During the Romanization, the Region of my case study presents a complexity of patterns, as result of the interaction of local needs and backgrounds with the global process of institutional and economic unification of the Mediterranean following. The Social Network Analysis (SNA) has been chosen as methodology to approach this complexity. I propose some parameters to detect local identity markers, then I present an integrated method to relate their development to the evolution of the relational networks between the communities.},
	language = {en},
	number = {2},
	urldate = {2018-09-11},
	journal = {Digital Classics Online},
	author = {Vela, Raffaella Da},
	month = nov,
	year = {2017},
