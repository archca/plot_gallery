@article{reitmaier_alpine_2018,
	series = {Casting a glance over the mountain – multi-proxy approaches to the understanding of vertical mobility},
	title = {Alpine cattle management during the {Bronze} {Age} at {Ramosch}-{Mottata}, {Switzerland}},
	volume = {484},
	issn = {1040-6182},
	url = {http://www.sciencedirect.com/science/article/pii/S1040618216305134},
	doi = {10.1016/j.quaint.2017.02.007},
	abstract = {Based on a series of new radiocarbon dates we examine the vertical mobility of cattle in the Alps by means of strontium isotope analysis on samples from the prehistoric settlement of Ramosch-Mottata (Canton of Grisons, Switzerland). By identifying variations in the strontium isotope ratios of high-crowned cattle molars, we investigate the seasonal use of alpine pastures (vertical transhumance) and changes in cattle husbandry practices between the early and later stages of the site's occupation. Combined with the evidence of multiple high-altitude sites, indications of dairying and ethno-archeological observations, we see an economic shift and a reorganization of domestic animal exploitation from the early to the late Bronze/early Iron Age in the Alps.},
	urldate = {2018-08-21},
	journal = {Quaternary International},
	author = {Reitmaier, Thomas and Doppler, Thomas and Pike, Alistair W. G. and Deschler-Erb, Sabine and Hajdas, Irka and Walser, Christoph and Gerling, Claudia},
	month = aug,
	year = {2018},
	keywords = {Mobility, Strontium isotopes, Prehistory, Alps, Diagenesis, LA-MC-ICP-MS},
	pages = {19--31},
	file = {Reitmaier et al_2018_Alpine cattle management during the Bronze Age at Ramosch-Mottata, Switzerland.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/48ALPWAU/Reitmaier et al_2018_Alpine cattle management during the Bronze Age at Ramosch-Mottata, Switzerland.pdf:application/pdf;ScienceDirect Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/Z72CR28L/S1040618216305134.html:text/html}
