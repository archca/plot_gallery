	file = {Bonsall et al_2015_New AMS 14C Dates for Human Remains from Stone Age Sites in the Iron Gates.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/3L6WNAC6/Bonsall et al_2015_New AMS 14C Dates for Human Remains from Stone Age Sites in the Iron Gates.pdf:application/pdf;Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/ZS2UPHL5/8B9E5D728E6DD334EB8A52FC3B1D5FE0.html:text/html}
