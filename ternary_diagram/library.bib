@article{haughton_social_2018,
	title = {Social {Relations} and the {Local}: {Revisiting} {Our} {Approaches} to {Finding} {Gender} and {Age} in {Prehistory}. {A} {Case} {Study} from {Bronze} {Age} {Scotland}},
