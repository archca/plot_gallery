	journal = {Journal of World Prehistory},
	author = {Shennan, Stephen J.},
	month = jun,
	year = {1993},
	pages = {121--161},
	file = {10.1007%2FBF00975449:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/TQYV48C3/10.1007%2FBF00975449.pdf:application/pdf}
