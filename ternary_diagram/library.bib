@article{rey_radiocarbon_2019,
	title = {Radiocarbon {Wiggle} {Matching} on {Laminated} {Sediments} {Delivers} {High}-{Precision} {Chronologies}},
	volume = {61},
	issn = {0033-8222, 1945-5755},
	url = {https://doi.org/10.1017/RDC.2018.47},
	doi = {10.1017/RDC.2018.47},
	abstract = {High-resolution sediment chronologies with the best possible time control are essential for comparing palaeoecological studies with independent high-precision climatic, archaeological or historic data in order to disentangle causes and effects of past environmental, ecological and societal change. We present two varved lake sediment sequences from Moossee and Burgäschisee (Swiss Plateau) that have chronologies developed with Bayesian models and radiocarbon (14C) dating of terrestrial plant macrofossils extracted from sediment samples with constant age ranges. We illustrate the potential of high-resolution 14C dating for the construction of robust, high-precision sediment chronologies. The mean 2σ age uncertainties were reduced to±19 cal yr for Moossee and to±54 cal yr for Burgäschisee over the entire period of 3000 cal yr, while 2σ uncertainties of only±13 cal yr and±18 cal yr respectively, were achieved for shorter time intervals. These precisions are better than or comparable to those of previous varve studies. Our results imply that a sophisticated subsampling strategy and a careful selection of short-lived and well-defined terrestrial plant remains are crucial to avoid outlying 14C ages. A direct linkage between palaeoeological studies with dendrochronologically dated, local archaeological sites as well as a precise comparison with high-resolution climate proxy data have become feasible.},
	language = {en},
	number = {1},
	urldate = {2019-02-14},
	journal = {Radiocarbon},
	author = {Rey, Fabian and Gobet, Erika and Szidat, Sönke and Lotter, André F. and Gilli, Adrian and Hafner, Albert and Tinner, Willy},
	month = feb,
	year = {2019},
	keywords = {age-depth modeling, AMS dating, OxCal, V-sequence, varved lake sediments},
	pages = {265--285},
	file = {Rey et al_2019_Radiocarbon Wiggle Matching on Laminated Sediments Delivers High-Precision.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/QQGID2TQ/Rey et al_2019_Radiocarbon Wiggle Matching on Laminated Sediments Delivers High-Precision.pdf:application/pdf;Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/75QQ5IRF/A42E465D99B5ED2ECF9CCB0318CFDEAC.html:text/html}
}

