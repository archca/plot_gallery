@article{paulsson_radiocarbon_2019,
	title = {Radiocarbon dates and {Bayesian} modeling support maritime diffusion model for megaliths in {Europe}},
	volume = {116},
	copyright = {Copyright © 2019 the Author(s). Published by PNAS.. This open access article is distributed under Creative Commons Attribution-NonCommercial-NoDerivatives License 4.0 (CC BY-NC-ND).},
	issn = {0027-8424, 1091-6490},
	url = {https://doi.org/10.1073/pnas.1813268116},
	doi = {10.1073/pnas.1813268116},
	abstract = {There are two competing hypotheses for the origin of megaliths in Europe. The conventional view from the late 19th and early 20th centuries was of a single-source diffusion of megaliths in Europe from the Near East through the Mediterranean and along the Atlantic coast. Following early radiocarbon dating in the 1970s, an alternative hypothesis arose of regional independent developments in Europe. This model has dominated megalith research until today. We applied a Bayesian statistical approach to 2,410 currently available radiocarbon results from megalithic, partly premegalithic, and contemporaneous nonmegalithic contexts in Europe to resolve this long-standing debate. The radiocarbon results suggest that megalithic graves emerged within a brief time interval of 200 y to 300 y in the second half of the fifth millennium calibrated years BC in northwest France, the Mediterranean, and the Atlantic coast of Iberia. We found decisive support for the spread of megaliths along the sea route in three main phases. Thus, a maritime diffusion model is the most likely explanation of their expansion.},
	language = {en},
	number = {9},
	urldate = {2019-03-11},
	journal = {Proceedings of the National Academy of Sciences},
	author = {Paulsson, B. Schulz},
	month = feb,
	year = {2019},
	pmid = {30808740},
	keywords = {radiocarbon dates, Bayesian analysis, mobility, megaliths, megalithic seafaring},
	pages = {3460--3465},
	file = {Paulsson_2019_Radiocarbon dates and Bayesian modeling support maritime diffusion model for.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/8T4KEHM2/Paulsson_2019_Radiocarbon dates and Bayesian modeling support maritime diffusion model for.pdf:application/pdf;Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/TJBCB4FT/3460.html:text/html}
}

@article{szidat_radiocarbon_2017,
	title = {Radiocarbon {Dating} of {Bones} at the {LARA} {Laboratory} in {Bern}, {Switzerland}},
	volume = {59},
	issn = {0033-8222, 1945-5755},
	url = {https://doi.org/10.1017/RDC.2016.90},
	doi = {10.1017/RDC.2016.90},
	abstract = {The Laboratory for the Analysis of Radiocarbon with AMS (LARA) was established at the University of Bern in 2013. Since then, the quality of sample preparation and radiocarbon measurement procedures have been validated for different materials such as plant remains, macrofossils, bulk sediment, charcoals, and wood. This article presents the optimization of sample pretreatment of bones based on protocols described in the literature. The extraction of collagen was performed with an acid-base-acid treatment, gelatinization, coarse filtration, lyophilization, and graphitization. Dating results were validated as satisfactory for the Holocene by investigation of 36 individual samples previously dated by other 14C laboratories including VIRI/SIRI materials and from well-known archaeological contexts. An additional blank contribution for bone treatment was determined by comparison with the preparation of other sample materials. The cases of two outliers from the Holocene were studied in detail.},
	language = {en},
	number = {3},
	urldate = {2019-03-11},
	journal = {Radiocarbon},
	author = {Szidat, Sönke and Vogel, Edith and Gubler, Regula and Lösch, Sandra},
	month = jun,
	year = {2017},
	keywords = {chemical analysis, bone, pretreatment, 14C AMS dating},
	pages = {831--842},
	file = {Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/GGIXUMLH/2098CAC7A0CA8A0E0A5502E47F746E25.html:text/html;Szidat et al_2017_Radiocarbon Dating of Bones at the LARA Laboratory in Bern, Switzerland.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/Y8JA5JY2/Szidat et al_2017_Radiocarbon Dating of Bones at the LARA Laboratory in Bern, Switzerland.pdf:application/pdf}
}

