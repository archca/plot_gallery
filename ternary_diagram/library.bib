	author = {Canuto, Marcello A. and Estrada-Belli, Francisco and Garrison, Thomas G. and Houston, Stephen D. and Acuña, Mary Jane and Kováč, Milan and Marken, Damien and Nondédéo, Philippe and Auld-Thomas, Luke and Castanet, Cyril and Chatelain, David and Chiriboga, Carlos R. and Drápela, Tomáš and Lieskovský, Tibor and Tokovinine, Alexandre and Velasquez, Antolín and Fernández-Díaz, Juan C. and Shrestha, Ramesh},
	month = sep,
