	pages = {860--875},
	file = {ScienceDirect Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/IIK9E7PB/S2352409X16304783.html:text/html;Sureda et al_2017_Surviving on the isle of Formentera (Balearic Islands).pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/4B8RBJL7/Sureda et al_2017_Surviving on the isle of Formentera (Balearic Islands).pdf:application/pdf}
