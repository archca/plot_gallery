@article{van_den_berg_greater_2015,
	title = {Greater than the sum of its parts? {Italo}-{Aegean} network dynamics in {Achaia} and the {Argolid} during the {Bronze} {Age}-{Iron} {Age} transition (ca. 1250-1000 {BC})},
	url = {https://www.academia.edu/31429744/},
	abstract = {This paper presents a comparison of the evidence for Italo-Aegean relations between Achaia and the Argolid during the Bronze Age-Iron Age transition. Drawing on network theory, it seeks to explain how these areas remained connected with Italy during the twelfth century BC crisis. To this aim, a multi-scalar analysis is conducted of the so-called ‘Urnfield’ bronzes. First, the diachronic and spatial distribution of these bronzes is examined for each region in order to obtain a general picture of network dynamics and to identify regional hubs. Second, selected contexts are analyzed as a means to consider the dynamics and hubs at the local level. Third, the local and regional scales are confronted as a means to fully reconstruct Italo-Aegean networks. The analysis indicates that despite some generic points of convergence at the regional scale, network communities in Achaia and the Argolid had their local strategies for remaining connected with Italy.},
	number = {54},
	journal = {Tijdschrift voor Mediterrane Archeologie},
	author = {van den Berg, Kimberley},
	year = {2015},
	file = {van den Berg_2015_Greater than the sum of its parts.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/87IYHIKG/van den Berg_2015_Greater than the sum of its parts.pdf:application/pdf}
}

