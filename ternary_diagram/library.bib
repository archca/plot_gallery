	year = {2014},
	pages = {401--421},
	file = {Deckers et al_2014_Radiocarbon Chronology of Early Bronze Age Sites in the Middle Euphrates.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/C2W785SQ/Deckers et al_2014_Radiocarbon Chronology of Early Bronze Age Sites in the Middle Euphrates.pdf:application/pdf}
}

@article{maziar_communication_2017,
	title = {Communication networks and economical interactions: {Sourcing} obsidian in the {Araxes} {River} basin},
	volume = {14},
	issn = {2352-409X},
	shorttitle = {Communication networks and economical interactions},
	url = {http://www.sciencedirect.com/science/article/pii/S2352409X17301967},
	doi = {10.1016/j.jasrep.2017.05.021},
	abstract = {There has been a long discussion on the nature of cultural, societal, and economical transformation after the migration of the Kura-Araxes groups from southern Caucasus to Iran and eastern Anatolia in the host areas. Provenance studies of obsidian artifacts could shed more light on this issue from another aspect. In this paper, we investigate if there is any similarity or difference in the obsidian consumption model among Kura-Araxes and autochthonous groups and if these similarities or differences can say anything about the economic, social, and cultural management of theses societies and their inner societal and cultural dynamics. A total of forty-eight obsidian artifacts from 13 surveyed and two excavated sites were analyzed by X-ray fluorescence. The results show the cultural transformation in northwestern Iran did not affect the exchange areas and interaction routes.},
	urldate = {2019-04-01},
	journal = {Journal of Archaeological Science: Reports},
	author = {Maziar, Sepideh and Glascock, Michael D.},
	month = aug,
	year = {2017},
	keywords = {Obsidian, Migration, Araxes River basin, Kura-Araxes cultural tradition, Northwestern Iran, Syunik},
	pages = {31--37},
	file = {Maziar_Glascock_2017_Communication networks and economical interactions.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/YIR2ZII9/Maziar_Glascock_2017_Communication networks and economical interactions.pdf:application/pdf;ScienceDirect Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/SJ2WIEIC/S2352409X17301967.html:text/html}
}

@article{van_beek_landscape_2019,
	title = {The landscape setting of bog bodies: {Interdisciplinary} research into the site location of {Yde} {Girl}, {The} {Netherlands}},
	issn = {0959-6836},
	shorttitle = {The landscape setting of bog bodies},
	url = {https://doi.org/10.1177/0959683619838048},
	doi = {10.1177/0959683619838048},
	abstract = {Past studies of archaeological bog finds, such as bog bodies, wooden trackways and a wide variety of other materials, are characterized by a strong focus on material culture. Their original environmental and cultural context has received far less attention. This paper centres on the original landscape setting of bog bodies. Interdisciplinary reconstructions of the physical and cultural landscape at the time of deposition can lead to significant new and more detailed insights into the context and meaning of this remarkable phenomenon. We aim to show the value of such interdisciplinary research by reconstructing the original physical and cultural landscape setting of the most iconic bog body of The Netherlands: Yde Girl. This approximately 16-year-old girl was killed about 2000 years ago and deposited in a bog south of the modern-day village of Yde (province of Drenthe). Our interdisciplinary research team used a combination of research methods from physical geography, geomorphology, palynology and archaeology to analyse both the site itself and its wider environment. This kind of integrated, detailed landscape research on bog bodies has hardly been done yet. We expect that our research design, methodology and results may also be applied in future research of other bog bodies. Furthermore, they may inspire research on other types of archaeological find categories from peatlands.},
	language = {en},
	urldate = {2019-04-01},
	journal = {The Holocene},
	author = {van Beek, R and Candel, JHJ and Quik, C and Bos, JAA and Gouw-Bouman, MTIJ and Makaske, B and Maas, GJ},
	month = apr,
	year = {2019},
	pages = {0959683619838048},
	file = {van Beek et al_2019_The landscape setting of bog bodies.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/YXVM4YNH/van Beek et al_2019_The landscape setting of bog bodies.pdf:application/pdf}
}

@article{zazzo_radiocarbon_2009,
	title = {Radiocarbon {Dating} of {Calcined} {Bones}: {Where} {Does} the {Carbon} {Come} from?},
	volume = {51},
	issn = {0033-8222, 1945-5755},
	shorttitle = {Radiocarbon {Dating} of {Calcined} {Bones}},
	url = {https://doi.org/10.1017/S0033822200055958},
	doi = {10.1017/S0033822200055958},
	abstract = {Over the past decade, radiocarbon dating of the carbonate contained in the mineral fraction of calcined bones has emerged as a viable alternative to dating skeletal remains in situations where collagen is no longer present. However, anomalously low δ13C values have been reported for calcined bones, suggesting that the mineral fraction of bone is altered. Therefore, exchange with other sources of carbon during heating cannot be excluded. Here, we report new results from analyses on cremated bones found in archaeological sites in Africa and the Near East, as well as the results of several experiments aiming at improving our understanding of the fate of mineral and organic carbon of bone during heating. Heating of modern bone was carried out at different temperatures, for different durations, and under natural and controlled conditions, and the evolution of several parameters (weight, color, \%C, \%N, δ13C value, carbonate content, crystallinity indexes measured by XRD and FTIR) was monitored. Results from archaeological sites confirm that calcined bones are unreliable for paleoenvironmental and paleodietary reconstruction using stable isotopes. Experimental results suggest that the carbon remaining in bone after cremation likely comes from the original inorganic pool, highly fractionated due to rapid recrystallization. Therefore, its reliability for 14C dating should be seen as close to that of tooth enamel, due to crystallographic properties of calcined bones.},
	language = {en},
	number = {2},
	urldate = {2019-04-02},
	journal = {Radiocarbon},
	author = {Zazzo, A. and Saliège, J.-F. and Person, A. and Boucher, H.},
	year = {2009},
	pages = {601--611},
	file = {Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/MH8V2EXU/E6BEE893F2A698D1BE8EFB149B1182AC.html:text/html;Zazzo et al_2009_Radiocarbon Dating of Calcined Bones.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/BIEKMZBI/Zazzo et al_2009_Radiocarbon Dating of Calcined Bones.pdf:application/pdf}
}

