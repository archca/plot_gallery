	keywords = {Scandinavia, Bayesian approach, Neolithization},
	pages = {583--593},
	file = {Paulsson et al_2017_A Highly Precise Chronology for the Process of Neolithization in Southern.pdf:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/USJ8MLQ6/Paulsson et al_2017_A Highly Precise Chronology for the Process of Neolithization in Southern.pdf:application/pdf;Snapshot:/Volumes/LaCie/Dokumente/Archäologie/Zotero/storage/C8Z4MRTX/7B8FAF3AD786A0F38A218F58995549E2.html:text/html}
