	journal = {Journal of Archaeological Science: Reports},
	author = {Sureda, Pau and Camarós, Edgard and Cueto, Marian and Teira, Luís C. and Aceituno, Francisco J. and Albero, Daniel and Álvarez-Fernández, Esteban and Bofill, Maria and López-Dóriga, Inés and Marín, Dioscórides and Masclans, Alba and Picornell, Llorenç and Revelles, Jordi and Burjachs, Francesc},
	month = apr,
